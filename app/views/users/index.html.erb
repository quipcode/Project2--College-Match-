<h1>Sign in or signup</h1>



<% if current_user %>

  <ul>

    <% @users.each do |x| %>
        <li><%= link_to x.email, user_path(x) , method: :get %></li>
    <% end %>

  </ul>
  <%= link_to 'logout', logout_path, method: :delete %>

<% else %>

  <a href="users/new">Register</a>
  <a href="login">Login</a>

<% end %>
