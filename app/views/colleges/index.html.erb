<h1>Colleges#index</h1>
<p>Find me in app/views/colleges/index.html.erb</p>

<% if current_user && @colleges["result"]["records"].include?(@desired_college) %>
  <% @colleges["result"]["records"].each do |record| %>
    <%= record if record["INSTNM"] == @desired_college %>
  <% end %>
  <div>
    <p>is the college you wish to attend </p>
    <button>Yes</button>
  </div>
<% else %>
  <p>Error go back to your home page</p>
  <%= link_to 'Back', :back %>
<% end %>
